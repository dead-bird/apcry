{
  "name": "apcry",
  "version": "1.0.0",
  "description": "APCry is a cry-typing generator and open API for creating maximum tears on the fly",
  "main": "index.js",
  "scripts": {
    "prestart": "",
    "api:dev": "nodemon --exec babel-node api/index.js",
    "api:serve": "npm run api:build && pm2 start dist/index.js --name apcry",
    "api:build": "babel api -d dist",
    "app:dev": "webpack-dev-server --config webpack.dev.js",
    "app:build": "webpack --config webpack.live.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dead-bird/apcry.git"
  },
  "keywords": [
    "crytext",
    "crytype",
    "crytyping",
    "crytext",
    "crytyper",
    "cry-type",
    "cry-typing",
    "cry-text",
    "cry-typer",
    "generator",
    "API"
  ],
  "readmeFilename": "README.md",
  "license": "MIT License",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.3.1",
    "nodemon": "^1.18.9",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.13.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-loader": "^8.0.6",
    "bad-words": "^3.0.2",
    "bootstrap": "^4.3.1",
    "chalk": "^2.4.2",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "highlight.js": "^9.15.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-persist": "^3.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^0.23.1",
    "twemoji": "^12.1.2",
    "twitter": "^1.7.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webmanifest-loader": "^0.3.0"
  },
  "engines": {
    "node": ">=6"
  }
}
